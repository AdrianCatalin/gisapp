{
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/docdash",
    "destination": "../docs/",
    "recurse": true,
    "verbose": true
  },
  "docdash": {
    "static": true,
    "sort": false,
    "search": true,
    "menu": {
      "GitHub repository": {
        "href":"https://github.com/uprel/gisapp",
        "target":"_blank",
        "class":"menu-item",
        "id":"website_link"
      }
    }
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }}